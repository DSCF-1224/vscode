{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

	// unicode-math
	"unimath-symbol: \\lparen":    { "prefix": "lparen",    "body": "\\lparen $0",    "description": "unimath-symbol\nUSV : U+00028\nopening symbol\n"     },
	"unimath-symbol: \\rparen":    { "prefix": "rparen",    "body": "\\rparen $0",    "description": "unimath-symbol\nUSV : U+00029\nclosing symbol\n"     },
	"unimath-symbol: \\mathcomma": { "prefix": "mathcomma", "body": "\\mathcomma $0", "description": "unimath-symbol\nUSV : U+0002C\npunctuation symbol\n" },
	"unimath-symbol: \\mathplus":  { "prefix": "mathplus",  "body": "\\mathplus $0",  "description": "unimath-symbol\nUSV : U+0002B\nbinary symbol\n"      },
	"unimath-symbol: \\less":      { "prefix": "less",      "body": "\\less $0",      "description": "unimath-symbol\nUSV : U+0003C\nrelation symbol\n"    },
	"unimath-symbol: \\equal":     { "prefix": "equal",     "body": "\\equal $0",     "description": "unimath-symbol\nUSV : U+0003D\nrelation symbol\n"    },
	"unimath-symbol: \\greater":   { "prefix": "greater",   "body": "\\greater $0",   "description": "unimath-symbol\nUSV : U+0003E\nrelation symbol\n"    },
	"unimath-symbol: \\lbrace":    { "prefix": "lbrace",    "body": "\\lbrace $0",    "description": "unimath-symbol\nUSV : U+0007B\nopening symbol\n"     },
	"unimath-symbol: \\rbrace":    { "prefix": "rbrace",    "body": "\\rbrace $0",    "description": "unimath-symbol\nUSV : U+0007D\nclosing symbol\n"     },
	"unimath-symbol: \\minus":     { "prefix": "minus",     "body": "\\minus $0",     "description": "unimath-symbol\nUSV : U+02212\nbinary symbol\n"      },
	"unimath-symbol: \\therefore": { "prefix": "therefore", "body": "\\therefore $0", "description": "unimath-symbol\nUSV : U+02234\nordinary symbol\n"    },
	"unimath-symbol: \\coloneq":   { "prefix": "coloneq",   "body": "\\coloneq $0",   "description": "unimath-symbol\nUSV : U+02254\nrelation symbol\n"    },
	"unimath-symbol: \\cdot":      { "prefix": "cdot",      "body": "\\cdot $0",      "description": "unimath-symbol\nUSV : U+022C5\nbinary symbol\n"      },

	// private comment 
	"private comment\nheader":{
		"prefix": "comment",
		"body": "$LINE_COMMENT --- $1 --- $LINE_COMMENT\n$0"
	},

	// memo
	"Reynolds"             :{"prefix": "Reynolds", "body": "Reynolds"},
	"Reynolds number"      :{"prefix": "Reynolds", "body": "Reynolds数"},
		"High Reynolds number" :{"prefix": "Reynolds", "body": "高Reynolds数"},
		"Low Reynolds number"  :{"prefix": "Reynolds", "body": "低Reynolds数"},

	// determined by the document style //
	"\\bigskip"  :{ "prefix": "\\bigskip",   "body": "\\bigskip$LINE_COMMENT",   "description": "This command is equivalent to \\vspace{bigskipamount}\nwhere `bigskipamount` is determined by the document style.\n" },
	"\\medskip"  :{ "prefix": "\\medskip",   "body": "\\medskip$LINE_COMMENT",   "description": "This command is equivalent to \\vspace{medskipamount}\nwhere `medskipamount` is determined by the document style.\n" },
	"\\smallskip":{ "prefix": "\\smallskip", "body": "\\smallskip$LINE_COMMENT", "description": "This command is equivalent to \\vspace{smallskipamount}\nwhere `smallskipamount` is determined by the document style.\n" },


	// [beamer] class
		
		// Section 8 // Creating Frames

			// Section 8.1 // The Frame Environment

				"\\frame Environment":{
					"prefix": "\\frame",
					"body": [
						"\\begin{frame}<${1:overlay specification}>[${2:default overlay specification}][${3:options}]{${4:title}}{${5:subtitle}}",
						"\t${0:environment contents}",
						"\\end{frame}",
					],
					"description": [
						"⟨beamer⟩ Class",
						"Inserts the current page number into a template.",
						"beameruserguide.pdf p.064"
					]
				},

			// Section 8.2 // Components of a Frame

				// Section 8.2.1 // The Headline and Footline

				"\\insertpagenumber":{
					"prefix": "beamer \\insertpagenumber",
					"body": "\\insertpagenumber",
					"description": [
						"⟨beamer⟩ Class",
						"Inserts the current page number into a template.",
						"beameruserguide.pdf p.064"
					]
				},

				"\\insertsection":{
					"prefix": "beamer \\insertsection",
					"body": "\\insertsection",
					"description": [
						"⟨beamer⟩ Class",
						"Inserts the current section into a template.",
						"beameruserguide.pdf p.064"
					]
				},

				"\\insertshortauthor":{
					"prefix": "beamer \\insertshortauthor",
					"body": "\\insertshortauthor[${1:options}]",
					"description": [
						"⟨beamer⟩ Class",
						"Inserts the short version of the author into a template.",
						"beameruserguide.pdf p.064"
					]
				},

				"\\insertshortdate":{
					"prefix": "beamer \\insertshortdate",
					"body": "\\insertshortdate[${1:options}]",
					"description": [
						"⟨beamer⟩ Class",
						"Inserts the short version of the date into a template.",
						"beameruserguide.pdf p.064"
					]
				},

				"\\insertshortinstitute":{
					"prefix": "beamer \\insertshortinstitute",
					"body": "\\insertshortinstitute[${1:options}]",
					"description": [
						"⟨beamer⟩ Class",
						"Inserts the short version of the date into a template.",
						"beameruserguide.pdf p.065"
					]
				},

				"\\insertshortpart":{
					"prefix": "beamer \\insertshortpart",
					"body": "\\insertshortpart[${1:options}]",
					"description": [
						"⟨beamer⟩ Class",
						"Inserts the short version of the part name into a template.",
						"beameruserguide.pdf p.065"
					]
				},

				"\\insertshorttitle":{
					"prefix": "beamer \\insertshorttitle",
					"body": "\\insertshorttitle[${1:options}]",
					"description": [
						"⟨beamer⟩ Class",
						"Inserts the short version of the document title into a template.",
						"beameruserguide.pdf p.065"
					]
				},

				"\\insertshortsubtitle":{
					"prefix": "beamer \\insertshortsubtitle",
					"body": "\\insertshortsubtitle[${1:options}]",
					"description": [
						"⟨beamer⟩ Class",
						"Inserts the short version of the document subtitle into a template.",
						"beameruserguide.pdf p.065"
					]
				},

				"\\insertsubsection":{
					"prefix": "beamer \\insertsubsection",
					"body": "\\insertsubsection",
					"description": [
						"⟨beamer⟩ Class",
						"Inserts the current subsection into a template.",
						"beameruserguide.pdf p.065"
					]
				},

				"\\insertsubsubsection":{
					"prefix": "beamer \\insertsubsubsection",
					"body": "\\insertsubsubsection",
					"description": [
						"⟨beamer⟩ Class",
						"Inserts the current subsubsection into a template.",
						"beameruserguide.pdf p.065"
					]
				},

				"\\insertframenumber":{
					"prefix": "beamer \\insertframenumber",
					"body": "\\insertframenumber",
					"description": [
						"⟨beamer⟩ Class",
						"Inserts the number of the current frame (not slide) into a template.",
						"beameruserguide.pdf p.065"
					]
				},

				"\\insertslidenumber":{
					"prefix": "beamer \\insertslidenumber",
					"body": "\\insertslidenumber",
					"description": [
						"⟨beamer⟩ Class",
						"Inserts the number of the current slide in frame into a template.",
						"beameruserguide.pdf p.065"
					]
				},

				// Section 8.2.6 // The Frame Title

				"\\frametitle":{
					"prefix": "beamer \\frametitle",
					"body": "\\frametitle<${1:overlay specification}>[${2:<short frame title>}]{${3:<frame title text>}}",
					"description": "⟨beamer⟩ Class\nbeameruserguide.pdf p.073"
				},

				"\\framesubtitle":{
					"prefix": "beamer \\framesubtitle",
					"body": "\\framesubtitle<${1:overlay specification}>{${2:<frame title text>}}",
					"description": "⟨beamer⟩ Class\nbeameruserguide.pdf p.074"
				},

		// Section 9 // Creating Overlays

			// Section 9.3 // Commands with Overlay Specification

				"\\onslide":{
					"prefix": "beamer \\onslide",
					"body": "\\onslide${1:<modifiler>}<${2:overlay specification}>{${3:text}}",
					"description": "⟨beamer⟩ Class\nbeameruserguide.pdf p.080"
				},

				"\\only":{
					"prefix": "beamer \\only",
					"body": "\\only<${1:overlay specification}>{${2:text}}<${3:overlay specification}>",
					"description": "⟨beamer⟩ Class\nbeameruserguide.pdf p.080"
				},

				"\\uncover":{
					"prefix": "beamer \\uncover",
					"body": "\\uncover<${1:overlay specification}>{${2:text}}",
					"description": "⟨beamer⟩ Class\nbeameruserguide.pdf p.081"
				},

				"\\visible":{
					"prefix": "beamer \\visible",
					"body": "\\visible<${1:overlay specification}>{${2:text}}",
					"description": "⟨beamer⟩ Class\nbeameruserguide.pdf p.081"
				},

		// Section 12 // structuring a presentation: The Local structure

				// Section.12.5 // Framed and Boxed Text
				"beamer :⟨beamercolorbox⟩ environment":{
					"prefix": "beamer \\beamercolorbox",
					"body": [
						"\\begin{beamercolorbox}[${1:options}]{${2:beamer color}}",
						"\t${3:⟨environment contents⟩}",
						"\\end{beamercolorbox}",
					],
					"description": [
						":⟨beamer⟩ Class",
						"beameruserguide.pdf p.123"
					]
				},

					"beamer :⟨beamercolorbox⟩ environment: option: wd":{
						"prefix": "beamer \\beamercolorbox wd",
						"body": "wd=${1:⟨width⟩}",
						"description": [
							":⟨beamer⟩ Class",
							"1st default: \\hsize",
							"2nd default: the width of the box typeseted",
							"beameruserguide.pdf p.124"
						]
					},

				// Section.12.7 // splitting a frame into multiple columns
				"beamer <columns> environment option: onlytextwidth":{
					"prefix": "beamer columns: onlytextwidth",
					"body": "onlytextwidth, $0",
					"description": "⟨beamer⟩ Class\nbeameruserguide.pdf p.127"
				},


