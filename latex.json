	// [tikzpicture] environment

		"tikzpicture environment":{
			"prefix": "/tikz/environment",
			"body": [
				"\\begin{tikzpicture}",
				"\t$1",
				"\\end{tikzpicture}"
			]
		},

		"tikzpicture origin":{
			"prefix": "/tikz/origin",
			"body": "\\path ( 0.0, 0.0 ) node [name=origin, shape=rectangle, anchor=center, draw=none, fill=none] {};",
		},

		"tikzpicture node single":{
			"prefix": "/tikz/node",
			"body": "\\path ( , ) node [name={}, shape={}, anchor={}, draw=none, fill=none] {};",
		},

		"tikzpicture node on the path":{
			"prefix": "/tikz/node",
			"body": "node [pos={}, name={}, shape={}, anchor={}, draw=none, fill=none] {};",
		},

		"tikzpicture draw current bounding box":{
			"prefix": "/tikz/current bounding box",
			"body": " "
		},

			// Section.13 Specifying Coordinates //

				// Section.13.2 Coordinate Systems //
					
					// Section.13.2.3 Node Coordinate System //

						"tikzpicture: anchor":{
							"prefix": "/tikz/anchor",
							"body": "anchor=${1:⟨anchor⟩}, $0",
							"description": "no default\nSpecifies an anchor of the node.\nManual for Version 3.0.1a p.134\n"
						},

			// Section.14 //

				"tikzpicture: path: single line":{
					"prefix": "/tikz/path",
					"body": "\\path [${1:options}] ${2:⟨specification⟩};",
					"description": "Manual for Version 3.0.1a p.146\n"
				},

				"tikzpicture: path: multi line":{
					"prefix": "/tikz/path",
					"body": "\\path [${1:options}]$LINE_COMMENT\n\t${2:⟨specification⟩}\n;",
					"description": "Manual for Version 3.0.1a p.146\n"
				},

			// Section.15 Actions on Paths //

				// Section.15.2 Specifying a Color //

					"/tikz/color":{
						"prefix": "/tikz/color",
						"body": "color=${1:⟨color name⟩}, $0",
						"description": "no default\nManual for Version 3.0.1a p.165\n"
					},

				// Section.15.3 Drawing a Path //

					// 15.3.1 Graphic Parameters: Line Width, Line Cap, and Line Join //

						"/tikz/draw":{
							"prefix": "/tikz/draw",
							"body": "draw=${1:⟨color⟩}, $0",
							"description": "default is scope’s color setting\nManual for Version 3.0.1a p.165\n"
						},

						"/tikz/draw=none":{
							"prefix": "/tikz/draw none",
							"body": "draw=none, $0",
							"description": "default is scope’s color setting\nManual for Version 3.0.1a p.165\n"
						},

						"/tikz/line width":{
							"prefix": "/tikz/line width",
							"body": "line width=${1:⟨dimension⟩}, $0",
							"description": "no default\ninitially 0.4pt\nManual for Version 3.0.1a p.166\n"
						},

						"/tikz/ultra thin":{
							"prefix": "/tikz/ultra thin",
							"body": "ultra thin, $0",
							"description": "set the line width 0.1pt\nManual for Version 3.0.1a p.166\n"
						},

						"/tikz/very thin":{
							"prefix": "/tikz/very thin",
							"body": "very thin, $0",
							"description": "set the line width 0.2pt\nManual for Version 3.0.1a p.166\n"
						},

						"/tikz/thin":{
							"prefix": "/tikz/thin",
							"body": "thin, $0",
							"description": "set the line width 0.4pt\nManual for Version 3.0.1a p.166\n"
						},

						"/tikz/semithick":{
							"prefix": "/tikz/semithick",
							"body": "semithick, $0",
							"description": "set the line width 0.6pt\nManual for Version 3.0.1a p.166\n"
						},

						"/tikz/thick":{
							"prefix": "/tikz/thick",
							"body": "thick, $0",
							"description": "set the line width 0.8pt\nManual for Version 3.0.1a p.166\n"
						},

						"/tikz/very thick":{
							"prefix": "/tikz/very thick",
							"body": "very thick, $0",
							"description": "set the line width 1.2pt\nManual for Version 3.0.1a p.166\n"
						},

						"/tikz/ultra thick":{
							"prefix": "/tikz/ultra thick",
							"body": "ultra thick, $0",
							"description": "set the line width 1.6pt\nManual for Version 3.0.1a p.166\n"
						},

						"/tikz/line cap=butt":{
							"prefix": "/tikz/line cap butt",
							"body": "line cap=butt, $0",
							"description": "no default\ninitially `butt`\nManual for Version 3.0.1a p.167\n"
						},

						"/tikz/line cap=rect":{
							"prefix": "/tikz/line cap rect",
							"body": "line cap=rect, $0",
							"description": "no default\ninitially `butt`\nManual for Version 3.0.1a p.167\n"
						},

						"/tikz/line cap=round":{
							"prefix": "/tikz/line cap round",
							"body": "line cap=round, $0",
							"description": "no default\ninitially `butt`\nManual for Version 3.0.1a p.167\n"
