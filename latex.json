				// Section 8.2.6 // The Frame Title

				"\\frametitle":{
					"prefix": "beamer \\frametitle",
					"body": "\\frametitle<${1:<overlay specification>}>[${2:<short frame title>}]{${3:<frame title text>}}",
					"description": "<beamer> Class\nbeameruserguide.pdf p.073"
				},

				"\\framesubtitle":{
					"prefix": "beamer \\framesubtitle",
					"body": "\\framesubtitle<${1:<overlay specification>}>{${2:<frame title text>}}",
					"description": "<beamer> Class\nbeameruserguide.pdf p.074"
				},

