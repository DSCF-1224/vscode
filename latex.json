	// [beamer] class
		
		// Section 8 // Creating Frames

			// Section 8.1 // The Frame Environment

				"\\frame Environment":{
					"prefix": "\\frame",
					"body": [
						"\\begin{frame}<${1:<overlay specification>}>[${2:<default overlay specification>}][${3:<options>}]{${4:<title>}}{${5:subtitle}}",
						"\t${0:environment contents}",
						"\\end{frame}",
					],
					"description": "<beamer> Class\nInserts the current page number into a template.\nbeameruserguide.pdf p.064"
				},

			// Section 8.2 // Components of a Frame

				// Section 8.2.1 // The Headline and Footline

				"\\insertpagenumber":{
					"prefix": "beamer \\insertpagenumber",
					"body": "\\insertpagenumber",
					"description": "<beamer> Class\nInserts the current page number into a template.\nbeameruserguide.pdf p.064"
				},

				"\\insertsection":{
					"prefix": "beamer \\insertsection",
					"body": "\\insertsection",
					"description": "<beamer> Class\nInserts the current section into a template.\nbeameruserguide.pdf p.064"
				},

				"\\insertshortauthor":{
					"prefix": "beamer \\insertshortauthor",
					"body": "\\insertshortauthor[${1:<options>}]",
					"description": "<beamer> Class\nInserts the short version of the author into a template.\nbeameruserguide.pdf p.064"
				},

				"\\insertshortdate":{
					"prefix": "beamer \\insertshortdate",
					"body": "\\insertshortdate[${1:<options>}]",
					"description": "<beamer> Class\nInserts the short version of the date into a template.\nbeameruserguide.pdf p.064"
				},

				"\\insertshortinstitute":{
					"prefix": "beamer \\insertshortinstitute",
					"body": "\\insertshortinstitute[${1:<options>}]",
					"description": "<beamer> Class\nInserts the short version of the date into a template.\nbeameruserguide.pdf p.065"
				},

				"\\insertshortpart":{
					"prefix": "beamer \\insertshortpart",
					"body": "\\insertshortpart[${1:<options>}]",
					"description": "<beamer> Class\nInserts the short version of the part name into a template.\nbeameruserguide.pdf p.065"
				},

				"\\insertshortpart":{
					"prefix": "beamer \\insertshortpart",
					"body": "\\insertshortpart[${1:<options>}]",
					"description": "<beamer> Class\nInserts the short version of the part name into a template.\nbeameruserguide.pdf p.065"
				},

				"\\insertshorttitle":{
					"prefix": "beamer \\insertshorttitle",
					"body": "\\insertshorttitle[${1:<options>}]",
					"description": "<beamer> Class\nInserts the short version of the document title into a template.\nbeameruserguide.pdf p.065"
				},

				"\\insertshortsubtitle":{
					"prefix": "beamer \\insertshortsubtitle",
					"body": "\\insertshortsubtitle[${1:<options>}]",
					"description": "<beamer> Class\nInserts the short version of the document subtitle into a template.\nbeameruserguide.pdf p.065"
				},

				"\\insertsubsection":{
					"prefix": "beamer \\insertsubsection",
					"body": "\\insertsubsection",
					"description": "<beamer> Class\nInserts the current subsection into a template.\nbeameruserguide.pdf p.065"
				},

				"\\insertsubsubsection":{
					"prefix": "beamer \\insertsubsubsection",
					"body": "\\insertsubsubsection",
					"description": "<beamer> Class\nInserts the current subsubsection into a template.\nbeameruserguide.pdf p.065"
				},

				"\\insertframenumber":{
					"prefix": "beamer \\insertframenumber",
					"body": "\\insertframenumber",
