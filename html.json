	"MathJax\nomega"   : { "prefix": "@e", "body": "\\omega",   "description": "MathJax\nGreek letters\nsmall" },

	"MathJax\noverscript": {
		"prefix": "@overscript",
		"body": "{ $1 }^{ $2 }",
		"description": "MathJax\noverscript\n"
	}
