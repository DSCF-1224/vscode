{
	// Place your snippets for html here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

	// MathJax

	"MathJax\n<align> environment": {
		"prefix": "align",
		"body": [
			"\\begin{align}",
			"\t$0",
			"\\end{align}"
		],
		"description": "MathJax\n<align> environment\n"
	},

	"MathJax\n<align*> environment": {
		"prefix": "align*",
		"body": [
			"\\begin{align*}",
			"\t$0",
			"\\end{align*}"
		],
		"description": "MathJax\n<align*> environment\n"
	},

	"MathJax\n<alignat> environment": {
		"prefix": "alignat",
		"body": [
			"\\begin{alignat}{}",
			"\t$0",
			"\\end{alignat}"
		],
		"description": "MathJax\n<alignat> environment\n"
	},

	"MathJax\n<alignat*> environment": {
		"prefix": "alignat*",
		"body": [
			"\\begin{alignat*}{}",
			"\t$0",
			"\\end{alignat*}"
		],
		"description": "MathJax\n<alignat*> environment\n"
	},

	"MathJax\n<alignedat> environment": {
		"prefix": "alignedat",
		"body": [
			"\\begin{alignedat}{}",
			"\t$0",
			"\\end{alignedat}"
		],
		"description": "MathJax\n<alignedat> environment\n"
	},

	"MathJax\n<array> environment": {
		"prefix": "array",
		"body": [
			"\\begin{array}{$1}",
			"\t$0",
			"\\end{array}"
		],
		"description": "MathJax\n<array> environment\n"
	},

	"MathJax\n<equation> environment": {
		"prefix": "equation",
		"body": [
			"\\begin{equation}",
			"\t$0",
			"\\end{equation}"
		],
		"description": "MathJax\n<equation> environment\n"
	},

	"MathJax\n<equation*> environment": {
		"prefix": "equation*",
		"body": [
			"\\begin{equation*}",
			"\t$0",
			"\\end{equation*}"
		],
		"description": "MathJax\n<equation*> environment\n"
	},

	"MathJax\n\\approx": {
		"prefix": "approx",
		"body": "\\approx $0",
		"description": "MathJax\n≈\n"
	},

	"MathJax\n\\binom": {
		"prefix": "binom",
		"body": "\\binom{ $1 }{ $2 }",
		"description": "MathJax\nbinomial coefficient\n"
	},

	"MathJax\n\\displaystyle": {
		"prefix": "displaystyle",
		"body": "\\displaystyle",
		"description": "MathJax\nchange to the display style\n"
	},

	"MathJax\n\\frac{}{}": {
		"prefix": "frac",
		"body": "\\frac{$1}{$2}",
		"description": "MathJax\nfraction\n"
	},

	"MathJax\n\\lbrack": {
		"prefix": "lbrack",
		"body": "\\lbrack",
		"description": "MathJax\nleft square bracket\n"
	},

	"MathJax\n\\lbrace": {
		"prefix": "lbrace",
		"body": "\\lbrace",
		"description": "MathJax\nleft curly bracket\n"
	},

	"MathJax\n\\overbrace{}": {
		"prefix": "overbrace",
		"body": "\\overbrace{$1}",
		"description": "MathJax\n"
	},

	"MathJax\n\\overbrace{} with overscript": {
		"prefix": "overbrace",
		"body": "\\overbrace{$1}^{$2}",
		"description": "MathJax\n"
	},

	"MathJax\n\\overbrace{} with subscript": {
		"prefix": "overbrace",
		"body": "\\overbrace{$1}_{$2}",
		"description": "MathJax\n"
	},

	"MathJax\n\\overbrace{} with over/subscript": {
		"prefix": "overbrace",
		"body": "\\overbrace{$1}^{$2}_{$3}",
		"description": "MathJax\n"
	},

	"MathJax\n\\prime (double)": {
		"prefix": "prime",
		"body": "\\dprime",
		"description": "MathJax\n′\ndouble prime\n"
	},

	"MathJax\n\\prime (triple)": {
		"prefix": "prime",
		"body": "\\tprime",
		"description": "MathJax\n′\ntriple prime\n"
	},

	"MathJax\n\\mathrm{}": {
		"prefix": "mathrm",
		"body": "\\mathrm{$1}",
		"description": "MathJax\nRoman font style for math mode"
	},

	"MathJax\n\\mathtt{}": {
		"prefix": "mathtt",
		"body": "\\mathtt{$1}",
		"description": "MathJax\ntypewriter font style for math mode"
	},

	"MathJax\n\\prime": {
		"prefix": "prime",
		"body": "\\prime",
		"description": "MathJax\n′\nprime\n"
	},

	"MathJax\n\\quad": {
		"prefix": "quad",
		"body": "\\quad",
		"description": "MathJax\nequal to 1em space\n"
	},

	"MathJax\n\\qquad": {
		"prefix": "qquad",
		"body": "\\qquad",
		"description": "MathJax\nequal to 2em space\n"
	},

	"MathJax\n\\rbrack": {
		"prefix": "rbrack",
		"body": "\\rbrack",
		"description": "MathJax\nright square bracket\n"
	},

	"MathJax\n\\rbrace": {
		"prefix": "rbrace",
		"body": "\\rbrace",
		"description": "MathJax\nright curly bracket\n"
	},

	"MathJax\n\\text{}": {
		"prefix": "text",
		"body": "\\text{$1}",
		"description": "MathJax\nuse text mode in the math mode\n"
	},

	"MathJax\n\\underbrace{}": {
		"prefix": "underbrace",
		"body": "\\underbrace{$1}",
		"description": "MathJax\n"
	},

	"MathJax\n\\underbrace{} with overscript": {
		"prefix": "underbrace",
		"body": "\\underbrace{$1}^{$2}",
		"description": "MathJax\n"
	},

	"MathJax\n\\underbrace{} with subscript": {
		"prefix": "underbrace",
		"body": "\\underbrace{$1}_{$2}",
		"description": "MathJax\n"
	},

	"MathJax\n\\underbrace{} with over/subscript": {
		"prefix": "underbrace",
		"body": "\\underbrace{$1}^{$2}_{$3}",
		"description": "MathJax\n"
	},

	"MathJax\nparentheses": {
		"prefix": "parentheses",
		"body": "\\lparen $0 \\rparen",
		"description": "MathJax\n"
	},

