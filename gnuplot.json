{
/*
	// Place your snippets for Gnuplot here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	"Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Log output to console"
	}
*/

/* set datafile separator */
	"set datafile separator":{
		"prefix": "datafile separator",
		"body": "datafile separator '${0:chars}'",
		"description": "この後の入力ファイルのデータ列の分離文字が空白(whitespace)ではなくて，ここで指定する文字であるとgnuplotに支持します．"
	},

	"set datafile separator: whitespace":{
		"prefix": "datafile separator",
		"body": "datafile separator whitespace",
		"description": "この後の入力ファイルのデータ列の分離文字が空白(whitespace)ではなくて，ここで指定する文字であるとgnuplotに支持します．\n同コマンドのデフォルト値"
	},

	"set datafile separator: tab":{
		"prefix": "datafile separator",
		"body": "datafile separator tab",
		"description": "この後の入力ファイルのデータ列の分離文字が空白(whitespace)ではなくて，ここで指定する文字であるとgnuplotに支持します．\nタブ区切り"
	},

	"set datafile separator: comma":{
		"prefix": "datafile separator",
		"body": "datafile separator comma",
		"description": "この後の入力ファイルのデータ列の分離文字が空白(whitespace)ではなくて，ここで指定する文字であるとgnuplotに支持します．\nコンマ区切り"
	},

	"set datafile separator: several type":{
		"prefix": "datafile separator",
		"body": "datafile separator ${1:type1}|${2:type2}",
		"description": "この後の入力ファイルのデータ列の分離文字が空白(whitespace)ではなくて，ここで指定する文字であるとgnuplotに支持します．\nコンマ区切り"
	},

/* set format */

	"set format":{
		"prefix": "format",
		"body": "set format ${1:<axes>} ${2:\"<format-string>\"} ${3:{numeric|timedate|geographic}} # <axes> \"<format-string>\" {numeric|timedate|geographic}",
		"description": "軸の刻み書式 (format)\nVersion 5.0 patchlevel 6\n"
	},

	"set format: reset to default":{
		"prefix": "format (reset to default)",
		"body": "set format",
		"description": "軸の刻み書式 (format)\nVersion 5.0 patchlevel 6\n"
	},

/* set key */

	"set key: all":{
		"prefix": "key",
		"body": [
			"set key # {on|off} {default}",
			"set key # { {inside|outside} | {lmargin|rmargin|tmargin|bmargin} | {at <position>} }",
			"set key # {left|right|center} {top|bottom|center}",
			"set key # {vertical|horizontal} {Left|Right}",
			"set key # {noopaque|opaque}",
			"set key # {noreverse|reverse} {noinvert|invert}",
			"set key # {samplen <sample_length>} {spacing <vertical_spacing>}",
			"set key # {width <width_increment>} {height <height_increment>}",
			"set key # {noautotitle|autotitle {columnheader}}",
			"set key # {title \"<text>\"} {noenhanced|enhanced}",
			"set key # {font \"<face>,<size>\"} {textcolor <colorspec>}",
			"set key # {nobox|box {linestyle <style>|linetype <type>|linewidth <width>} }",
			"set key # {maxcols {<max no. of columns> | auto}}",
			"set key # {maxrows {<max no. of rows> | auto}}",
		],
		"description": "凡例 (key)\nVersion 5.0 patchlevel 6\n"
	},

/* set terminal */

	/* set terminal wxt */

		"set term: wxt":{
			"prefix": "term wxt",
			"body": [
				"set terminal wxt # {<n>}",
				"set terminal wxt # {size <width>,<height>} {position <x>,<y>}",
				"set terminal wxt # {background <rgb_color>}",
				"set terminal wxt # {noenhanced|enhanced}",
				"set terminal wxt # {font <font>} {fontscale <scale>}",
				"set terminal wxt # {title \"title\"}",
				"set terminal wxt # {linewidth <lw>}",
				"set terminal wxt # {dashlength <dl>}",
				"set terminal wxt # {{no}persist}",
				"set terminal wxt # {{no}raise}",
				"set terminal wxt # {{no}ctrl}",
				"set terminal wxt # {close}",
			],
			"description": "terminal wxt\nVersion 5.0 patchlevel 6\n"
		},

/* reset */
	"reset":{
		"prefix": "reset",
		"body": "reset",
		"description": "setコマンドで定義できる，グラフに関する全てのオプションをデフォルトの値に戻します．"
	},

	"reset session":{
		"prefix": "reset session",
		"body": "reset session",
		"description": "ユーザ定義変数、ユーザ定義関数すべてを削除し、デフォルトの設定を復帰し、システム全体の初期設定ファイル gnuplotrc と個人用の初期設定ファイル $HOME/.gnuplot を再実行します。"
	},

"every":{
	"prefix": "every",
	"body": "every $1:$2:$3:$4:$5:$6",
	"description": "描画するデータをデータ集合から周期的にサンプリングする"
},

"every pass toplines":{
	"prefix": "every",
	"body": "every ::${1:Lines}",
	"description": "描画するデータをデータ集合から周期的にサンプリングする"
},

