{
	// Place your snippets for vbscript here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

	// last updated : 2018/10/15

	// Function

		// CDbl Function
		// https://msdn.microsoft.com/ja-jp/library/cc410210.aspx
		"Function CDbl ":{
			"prefix": "CDbl",
			"body": "CDbl( ${0:expression} )",
			"description": "Function\n指定された式をバリアント型 (内部処理形式が倍精度浮動小数点数型 (Double) の Variant) に変換して返します。\n"
		},

		// CInt Function
		// https://msdn.microsoft.com/ja-jp/library/cc410210.aspx
		"Function CInt ":{
			"prefix": "CInt",
			"body": "CInt( ${0:expression} )",
			"description": "Function\n指定された式をバリアント型 (内部処理形式が整数型 (Integer) の Variant) に変換して返します。\n"
		},

		// CreateObject Function
		// https://msdn.microsoft.com/ja-jp/library/cc410215.aspx
		"CreateObject Function Simple":{
			"prefix": "CreateObject",
			"body": "CreateObject( ${1:servername}.${2:typename} )",
			"description": "Function\nオートメーション オブジェクトへの参照を作成して返します。"
		},
		// MsgBox Function
		// https://msdn.microsoft.com/ja-jp/library/cc410277.aspx
		// https://msdn.microsoft.com/ja-jp/library/sfw6660x.aspx
		"MsgBox Function simple":{
			"prefix": "MsgBox",
			"body": "${0:return value} = MsgBox( ${1:prompt} )",
			"description": "Function\nダイアログ ボックスにメッセージを表示し、ボタンがクリックされるのを待って、どのボタンがクリックされたかを示す値を返します。"
		},

		"MsgBox Function with buttons":{
			"prefix": "MsgBox",
			"body": "${0:return value} = MsgBox( ${1:prompt} , ${2:buttons} )",
			"description": "Function\nダイアログ ボックスにメッセージを表示し、ボタンがクリックされるのを待って、どのボタンがクリックされたかを示す値を返します。"
		},

		"MsgBox Function with buttons and title":{
			"prefix": "MsgBox",
			"body": "${0:return value} = MsgBox( ${1:prompt} , ${2:buttons} , ${3:title} )",
			"description": "Function\nダイアログ ボックスにメッセージを表示し、ボタンがクリックされるのを待って、どのボタンがクリックされたかを示す値を返します。"
		},

			// The buttons argument settings of MsgBox Function
			"Function MsgBox buttons: vbOKOnly":{
				"prefix": "MsgBox Button vbOKOnly",
				"body": "vbOKOnly",
				"description": "MsgBox Function\nDisplay [OK] button only.\ncorresponding value is 0\n"
			},

			"Function MsgBox buttons: vbOKCancel":{
				"prefix": "MsgBox Button vbOKCancel",
				"body": "vbOKCancel",
				"description": "MsgBox Function\nDisplay [OK] and [Cancel] buttons.\ncorresponding value is 1\n"
			},

			"Function MsgBox buttons: vbAbortRetryIgnore":{
				"prefix": "MsgBox Button vbAbortRetryIgnore",
				"body": "vbAbortRetryIgnore",
				"description": "MsgBox Function\nDisplay [Abort], [Retry], and [Ignore] buttons.\ncorresponding value is 2\n"
			},

			"Function MsgBox buttons: vbYesNoCancel":{
				"prefix": "MsgBox Button vbYesNoCancel",
				"body": "vbYesNoCancel",
				"description": "MsgBox Function\nDisplay [Yes], [No], and [Cancel] buttons.\ncorresponding value is 3\n"
			},

			"Function MsgBox buttons: vbYesNo":{
				"prefix": "MsgBox Button vbYesNo",
