	// Statements

		// Function Statement

			"ByVal (Function Statement)":{
				"prefix": "ByVal",
				"body": "ByVal ${1:varname}",
				"description": "Function Statement\nの引数\"varname\"が、値渡しで渡されることを示します。\n元のプロシージャーの値は変更されません。"
			},

			"ByRef (Function Statement)":{
				"prefix": "ByRef",
				"body": "ByRef ${1:varname}",
				"description": "Function Statement\nの引数\"varname\"が、値渡しで渡されることを示します。\n元のプロシージャーの値が変更される可能性があります。"
			},
		
		// Option Explicit Statement
		// https://msdn.microsoft.com/ja-jp/library/cc392455.aspx
		"Option Explicit Statement":{
			"prefix": "Option Explicit",
			"body": [
				"REM Forces explicit declaration of all variables in a script.",
				"Option Explicit",
			],
			"description": "Statement\nスクリプト内のすべての変数に対して、明示的な宣言を強制します。"
		},

		// Set Statement
		// https://msdn.microsoft.com/ja-jp/library/cc392465.aspx
		"Set Statement":{
			"prefix": "Set",
			"body": "SET ${1:objectvar} = ${2:{objectexpression|New classname|Nothing}}",
			"description": "Statement\nオブジェクトへの参照を変数またはプロパティに代入するか、プロシージャへの参照をイベントに関連付けます。"
		},

	// File System Object

		// def
		"declare the file system object":{
			"prefix": "fso",
			"body": [
				"REM # File System Object",
				"DIM obj_FSO",
				"SET obj_FSO = WScript.CreateObject(\"Scripting.FileSystemObject\")",
			],
		},

		// methods

			// CreateFolder Method
			// https://msdn.microsoft.com/ja-jp/library/cc427988.aspx
			"CreateFolder Method (File System Object)":{
				"prefix": "CreateFolder",
				"body": "CreateFolder( ${1:foldername} )",
				"description": "File System Object\nMethod\nフォルダを作成します。\n"
			},

			// FileExists Method
			// https://msdn.microsoft.com/ja-jp/library/cc428005.aspx
			"FileExists Method (File System Object)":{
				"prefix": "FileExists",
				"body": "${1:FileSystemObject}.FileExists( ${2:filespec} )",
				"description": "File System Object\nMethod\n指定したファイルが存在する場合は、真 (true) を返します。\n存在しない場合は、偽 (false) を返します。\n"
			},

			// FolderExists Method
			// https://msdn.microsoft.com/ja-jp/library/cc428005.aspx
			"FolderExists Method (File System Object)":{
				"prefix": "FolderExists",
				"body": "${1:FileSystemObject}.FolderExists( ${2:folderspec} )",
				"description": "File System Object\nMethod\n指定されたフォルダが存在する場合は、真 (true) を返します。\n存在しない場合は、偽 (false) を返します。\n"
			},

	// WScript

		// Echo Method
		"Echo Method (WScript Object)":{
			"prefix": "WScript.Echo",
			"body": "WScript.Echo ${0:args...}",
			"description": "WScript オブジェクト\nダイアログ ボックスまたはコンソールに出力を送ります。\n"
		},

		// WshShell Object

			// WshShell Object
			"Run Method (WshShell Object)":{
				"prefix": "WScript.WshShell Run",
				"body": "${1:WshShell object}.Run ${0:strCommand}",
				"description": "WScript オブジェクト\nWshShell オブジェクト\n新しいプロセス内でプログラムを実行します。\n"
			},

