{
	// Place your snippets for shellscript here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

	// gfortran

	// [reference]
	// - https://gcc.gnu.org/onlinedocs/gfortran/Fortran-Dialect-Options.html#Fortran-Dialect-Options
	"gfortran -ffree-line-length-none":{
		"prefix": "gfortran ffree line length none",
		"body": "-ffree-line-length-none $0",
		"description": "Set column after which characters are ignored in typical free-form lines in the source file.\n`none` means that the entire line length is meaningful.\n"
	},

	// [reference]
	// - https://gcc.gnu.org/onlinedocs/gfortran/Fortran-Dialect-Options.html#Fortran-Dialect-Options
	"gfortran -std=f95":{
		"prefix": "gfortran std f95",
		"body": "-std=f95 $0",
		"description": "Specify the standard to which the program is expected to conform.\nThe `f95` value specifies strict conformance to the Fortran 95.\n"
	},

	// [reference]
	// - https://gcc.gnu.org/onlinedocs/gfortran/Fortran-Dialect-Options.html#Fortran-Dialect-Options
	"gfortran -std=f2003":{
		"prefix": "gfortran std f2003",
		"body": "-std=f2003 $0",
		"description": "Specify the standard to which the program is expected to conform.\nThe `f2003` value specifies strict conformance to the Fortran 2008.\n"
	},

	// [reference]
	// - https://gcc.gnu.org/onlinedocs/gfortran/Fortran-Dialect-Options.html#Fortran-Dialect-Options
	"gfortran -std=f2008":{
		"prefix": "gfortran std f2008",
		"body": "-std=f2008 $0",
		"description": "Specify the standard to which the program is expected to conform.\nThe `f2008` value specifies strict conformance to the Fortran 2008.\n"
	},

	// [reference]
	// - https://gcc.gnu.org/onlinedocs/gfortran/Error-and-Warning-Options.html#Error-and-Warning-Options
	"gfortran -Wall":{
		"prefix": "gfortran Wall",
		"body": "-Wall $0",
		"description": "Enables commonly used warning options pertaining to usage that we recommend avoiding and that we believe are easy to avoid.\n"
	},

	// [reference]
	// - https://gcc.gnu.org/onlinedocs/gfortran/Error-and-Warning-Options.html#Error-and-Warning-Options
	"gfortran -Wextra":{
		"prefix": "gfortran Wextra",
		"body": "-Wextra $0",
		"description": "Enables some warning options for usages of language features which may be problematic.\n"
	},
}
